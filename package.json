{
  "name": "@mt-inc/server",
  "version": "1.0.4",
  "description": "Server for MT Inc. Project",
  "author": "Maksym Shamko",
  "license": "MIT",
  "private": true,
  "scripts": {
    "push": "git push origin master",
    "pull": "git pull origin master",
    "commit": "git add . && git commit -m 'working'",
    "server": "ts-node --files --project tsconfig.json ./",
    "pm:server": "pm2 start server.config.js",
    "pm:server:restart": "pm2 restart server.config.js",
    "pm:server:stop": "pm2 stop server.config.js",
    "pm:server:delete": "pm2 delete server.config.js"
  },
  "devDependencies": {
    "@mt-inc/strategy": "^1.0.20",
    "@types/express": "^4.17.13",
    "@types/node": "^16.10.3",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@mt-inc/utils": "^1.0.5",
    "express": "^4.17.1"
  },
  "packageManager": "yarn@3.1.1"
}
